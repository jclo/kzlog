  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/kzlog/issues"
  },
  "homepage": "https://github.com/jclo/kzlog",
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
