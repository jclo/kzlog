    "gulp-replace": "^1.1.3",
    "gulp-uglify-es": "^3.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "open": "^8.4.0",
    "opener": "^1.5.2",
    "sinon": "^12.0.1",
    "@mobilabs/es6lib": "1.0.12"
  },
