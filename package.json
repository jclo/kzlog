{
  "name": "@mobilabs/kzlog",
  "version": "0.0.0",
  "description": "A minimal lightweight logging library for JavaScript",
  "main": "index.js",
  "bin": {},
  "scripts": {
    "build": "gulp build",
    "watch": "gulp watch",
    "makedist": "gulp makedist",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha ./test/main.js",
    "check-coverage": "nyc check-coverage --statements 100 --branches 70 --functions 100 --lines 100",
    "display-coverage": "opener ./coverage/lcov-report/index.html",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "javascript",
    "logging"
  ],
  "author": {
    "name": "Jclo",
    "email": "jclo@mobilabs.fr",
    "url": "https://www.mobilabs.fr"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jclo/kzlog/issues"
  },
  "homepage": "https://github.com/jclo/kzlog",
  "dependencies": {},
  "devDependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "cross-env": "5.2.0",
    "del": "4.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.2",
    "gulp": "4.0.0",
    "gulp-concat": "2.6.1",
    "gulp-footer": "2.0.2",
    "gulp-header": "2.0.7",
    "gulp-replace": "1.0.0",
    "gulp-uglify": "3.0.2",
    "husky": "1.3.1",
    "mocha": "6.1.3",
    "nyc": "14.0.0",
    "opener": "1.5.1",
    "sinon": "7.3.2"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run makedist && npm run test && npm run check-coverage && npm run report"
    }
  }
}
